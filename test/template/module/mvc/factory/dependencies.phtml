<?php
namespace MyModule\Service;

use Zend\ServiceManager\Factory\FactoryInterface;
use Interop\Container\ContainerInterface;
use MyModule\Service\MyService;
use MyModule\Repository\DependencyOne;
use MyModule\Service\DependencyTwo;

/**
 * PHP Version 5
 *
 * @category Factory
 * @package MyModule/Service
 * @author Mauricio Piber <mauriciopiber@gmail.com>
 * @license GPL3-0 http://www.gnu.org/licenses/gpl-3.0.en.html
 * @link http://pibernetwork.com
 */
class MyServiceFactory implements FactoryInterface
{
    /**
     * Create MyService
     *
     * @param ContainerInterface $container ServiceManager instance
     *
     * @return MyService
     */
    public function __invoke(ContainerInterface $container, $requestedName = null, $options = [***REMOVED***)
    {
        $factory = new MyService(
            $container->get(DependencyOne::class),
            $container->get(DependencyTwo::class)
        );

        return $factory;
    }
}
