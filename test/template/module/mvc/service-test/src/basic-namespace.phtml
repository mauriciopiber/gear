 */
class BasicNamespaceServiceTest extends TestCase
{
    public function setUp()
    {
        parent::setUp();
        $this->service = new BasicNamespaceService();
    }

    public function testClassExists()
    {
        $this->assertInstanceOf('MyModule\Basic\BasicNamespaceService', $this->service);
    }
}
