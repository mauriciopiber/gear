<?php
namespace MyModuleTest\ControllerTest;

use MyModuleTest\ControllerTest\AbstractControllerTestCase;

/**
 * @group Controller
 */
class IndexControllerTest extends AbstractControllerTestCase
{
    public function setUp()
    {
        parent::setUp();
        $this->indexController = new \MyModule\Controller\IndexController();
    }

    public function testCanAccessNewCreatedModule()
    {
        $resp = $this->indexController->indexAction();
        $this->assertInstanceOf('Zend\View\Model\ViewModel', $resp);
    }
}
