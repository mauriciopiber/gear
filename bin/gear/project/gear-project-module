#!/bin/bash

functions="$( cd "$( dirname "${BASH_SOURCE[0***REMOVED***}" )" && cd ./../abstract/ && pwd )"

source "$functions/abstract-project.sh"
source "$functions/abstract-docs.sh"

worker="project-module"

source "$functions/init.sh"


if [ "$cmd" == "create" ***REMOVED***; then

    # Params
    if [ $# -lt 9 ***REMOVED***; then
        echo "usage $0: command project scriptsDir module type gearfile migration local ci"
        exit 1
    fi
   
    Gear_Project_Module_Create "${2}" "${3}" "${4}" "${5}" "${6}" "${7}" "${8}" "${9}"
	
	exit 0
fi

if [ "$cmd" == "construct" ***REMOVED***; then
	
	# Params
    if [ $# -lt 6 ***REMOVED***; then
        echo "usage $0: command project scriptsDir module gearfile migration local ci"
        exit 1
    fi
   
    Gear_Project_Module_Construct "${2}" "${3}" "${4}" "${5}" "${6}" "${7}" "${8}"
    
	exit 0
fi

if [ "$cmd" == "reset" ***REMOVED***; then
	
	# Params
    if [ $# -lt 5 ***REMOVED***; then
        echo "usage $0: command project module local ci"
        exit 1
    fi
   
    Gear_Project_Module_Reset "${2}" "${3}" "${4}" "${5}"

	exit 0
   
fi