#!/bin/bash

functions="$( cd "$( dirname "${BASH_SOURCE[0***REMOVED***}" )" && cd ./../abstract/ && pwd )"

source "$functions/abstract-module.sh"

worker="module"

source "$functions/init.sh"



if [ "$cmd" == "ci" ***REMOVED***; then

    # Params
    if [ $# -lt 2 ***REMOVED***; then
        echo "usage $0: command module type"
        exit 1
    fi
    
    Gear_Module_SetUpJenkins "${2}" "${3}"
	
	exit 0
fi


if [ "$cmd" == "diagnostic" ***REMOVED***; then

    # Params
    if [ $# -lt 2 ***REMOVED***; then
        echo "usage $0: command module type"
        exit 1
    fi
    
    Gear_Module_Execute_Diagnostic "${2}" "${3}"
	
	exit 0
fi

if [ "$cmd" == "custom" ***REMOVED***; then

    # Params
    if [ $# -lt 2 ***REMOVED***; then
        echo "usage $0: command module command-to-execute"
        exit 1
    fi
    
    Gear_Module_Execute_Custom "${2}" "${3}"
	
	exit 0
fi

if [ "$cmd" == "upgrade" ***REMOVED***; then

    # Params
    if [ $# -lt 2 ***REMOVED***; then
        echo "usage $0: command module type"
        exit 1
    fi
    
    Gear_Module_Execute_Upgrade "${2}" "${3}"
	
	exit 0
fi

if [ "$cmd" == "restore-module" ***REMOVED***; then

    # Params
    if [ $# -lt 2 ***REMOVED***; then
        echo "usage $0: command module type"
        exit 1
    fi
    
    Gear_Module_Execute_Restore_Module "${2}" "${3}"
	
	exit 0
fi


if [ "$cmd" == "restore-data" ***REMOVED***; then
    # Params
    if [ $# -lt 2 ***REMOVED***; then
        echo "usage $0: command module type"
        exit 1
    fi
    
    Gear_Module_Execute_Restore_Data "${2}" "${3}"
	
	exit 0
fi


if [ "$cmd" == "create" ***REMOVED***; then

    # Params
    if [ $# -lt 8 ***REMOVED***; then
        echo "usage $0: command module type scriptsDir construct local ci jira"
        exit 1
    fi
    
    Gear_Module_Execute_Create "${2}" "${3}" "${4}" "${5}" "${6}" "${7}" "${8}"
	
	exit 0
fi

if [ "$cmd" == "integrate" ***REMOVED***; then

    # Params
    if [ $# -lt 7 ***REMOVED***; then
        echo "usage $0: command module type scriptsDir construct local ci"
        exit 1
    fi
    
    Gear_Module_Execute_Integrate "${2}" "${3}" "${4}" "${5}" "${6}" "${7}"
	
	exit 0
fi

if [ "$cmd" == "construct" ***REMOVED***; then
	
	# Params
    if [ $# -lt 5 ***REMOVED***; then
        echo "usage $0: command module type scriptsDir construct local ci"
        exit 1
    fi
   
    Gear_Module_Execute_Construct "${2}" "${3}" "${4}" "${5}" "${6}" "${7}" 
    
	exit 0
fi

if [ "$cmd" == "clear" ***REMOVED***; then
	
	# Params
    if [ $# -lt 5 ***REMOVED***; then
        echo "usage $0: command module type local ci"
        exit 1
    fi
   
    Gear_Module_Execute_Clear "${2}" "${3}" "${4}" "${5}"

	exit 0
   
fi

if [ "$cmd" == "reset" ***REMOVED***; then
	
	# Params
    if [ $# -lt 4 ***REMOVED***; then
        echo "usage $0: command module local ci"
        exit 1
    fi
   
    Gear_Module_Execute_Reset "${2}" "${3}" "${4}"

	exit 0
   
fi

if [ "$cmd" == "build-suite" ***REMOVED***; then
	
    # Params
    if [ $# -lt 5 ***REMOVED***; then
        echo "usage $0: command module type scriptsDir construct"
        exit 1
    fi
    
    Gear_Module_Execute_Build_Suite "${2}" "${3}" "${4}" "${5}"
	   
    exit 0
fi


if [ "$cmd" == "test" ***REMOVED***; then
	
	# Params
    if [ $# -lt 4 ***REMOVED***; then
        echo "usage $0: command module type target"
        exit 1
    fi
   
    Gear_Module_Execute_Ant "${2}" "${3}" "${4}"

	exit 0

fi

if [ "$cmd" == "reconstruct" ***REMOVED***; then
	
	# Params
    if [ $# -lt 5 ***REMOVED***; then
        echo "usage $0: command module type scriptsDir construct local ci"
        exit 1
    fi
   
    Gear_Module_Execute_Reconstruct "${2}" "${3}" "${4}" "${5}" "${6}" "${7}" 
    
	exit 0

fi

