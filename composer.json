		"doctrine/doctrine-orm-module" : "0.8.*",
		"cypresslab/gitelephant" : "~1.0",
		"robmorgan/phinx" : "*",
		"doctrine/doctrine-module" : "0.8.*",
		"php" : ">=5.5",
		"mauriciopiber/gear-base" : "0.1.*",
