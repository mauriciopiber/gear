{
	"name" : "mauriciopiber/gear",
	"require" : {
		"doctrine/doctrine-orm-module" : "0.8.*",
		"cypresslab/gitelephant" : "~1.0",
		"robmorgan/phinx" : "*",
		"doctrine/doctrine-module" : "0.8.*",
		"php" : ">=5.3.3",
		"mauriciopiber/gear-base" : "0.1.*",
		"zendframework/zendframework" : "~2.5"
	},
	"require-dev" : {
		"sebastian/phpcpd" : "*",
		"sebastian/phpdcd" : "*",
		"phpunit/phpunit" : "4.*",
		"phploc/phploc" : "*",
